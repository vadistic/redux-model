  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.10",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@vadistic/eslint-config": "^0.5.0",
    "@vadistic/prettier-config": "^0.5.0",
    "eslint": "^7.11.0",
    "immer": "^7.0.9",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.1",
